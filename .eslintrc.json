{
	"extends": ["@react-native-community", "airbnb", "eslint-config-prettier"],
	"plugins": ["@typescript-eslint", "react"],
	"parser": "@typescript-eslint/parser",
	"rules": {
		"import/no-unresolved": 0,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".jsx", ".tsx"]
			}
		]
	}
}
